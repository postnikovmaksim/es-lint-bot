    "start": "probot run ./index.js",
    "lint": "standard --fix"
  },
  "dependencies": {
    "@moedelo/eslint-config-md": "^2.1.2",
    "eslint": "^5.6.1",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
